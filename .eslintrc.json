{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "no-restricted-syntax": "off",
    "no-plusplus": "off",
    "max-classes-per-file": "off",
    "no-incompatible-property-type": "off"
  }
}